[user]
  name = Fuad Saud
  email = fuadfsaud@gmail.com
  signingkey = C89D3162
[alias]
  a  = add --verbose
  au = update-index --assume-unchanged
  br = branch
  ci = commit --verbose
  co = checkout
  conflicts = diff --name-only --diff-filter=U
  cp = cherry-pick
  cpa = cherry-pick --abort
  cpc = cherry-pick --continue
  cry = cherry -v
  current-branch = symbolic-ref HEAD --short
  d  = diff
  dc = diff --cached
  dw = diff --word-diff
  f  = fetch --verbose --progress
  fix = ci --fixup HEAD
  fixup = ci --fixup
  fuck = "!git add .; git ci -m 'Fuck this shit, I am done for today!'; git p;"
  ft = fetch --verbose --tags
  fm = pull --verbose
  fr = pull --rebase --verbose
  frm = fr origin master
  l  = log --graph --abbrev-commit
  la = log --graph --abbrev-commit --all
  m  = merge --verbose
  nau = update-index --no-assume-unchanged
  p  = push --verbose --progress
  pr = pull-request
  prune-merged = "!git branch --merged | grep -v \\* | grep -v master | xargs -n 1 git branch -d"
  r  = rebase
  ra = rebase --abort
  rc = rebase --continue
  ri = rebase -i
  rias = rebase -i --autosquash
  redo = ci --amend --no-edit
  root = rev-parse --show-toplevel
  sd = stash drop
  sed  = "!git ls-files | xargs sed -i"
  sha = rev-parse
  sl = stash list
  snapshot = !git stash save "snapshot: $(date)" && git stash apply "stash@{0}"
  sp = stash pop
  ss = stash show -p
  st  = status --short --branch
  t = tag
  undo = reset HEAD~1
  unstage = reset HEAD
  untracked = ls-files --others --exclude-standard
  wc = whatchanged --graph --abbrev-commit
  wip = ci -m '•••••<[WIP]>•••••'
  gh-pages = !git checkout --orphan gh-pages && git rm $(git ls-files) -f
[color]
  ui = auto
[credential]
  helper = osxkeychain
[core]
  excludesfile = ~/.jarvis/git/ignore
[diff]
  algorithm = patience
[format]
  pretty = %C(yellow)%h •%Creset%C(red)%d%Creset %s %Cgreen• %cr by %cn%Creset
[github]
  user = fuadsaud
[hub]
  protocol = https
[merge]
  defaultToUpstream = true
[push]
  default = current
[url "https://"]
  insteadOf = git://
[gitsh]
  defaultCommand = status -sb
[gpg]
  program = gpg2
